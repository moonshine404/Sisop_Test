dd if=/dev/zero of=/home/mikogizka/Documents/disk_1 bs=500M count=1

mount /home/praktikan/Documents/disk_1 /media/test_fs
chown -R 1001:1001 /media/test_fs
cd /media/test_fs; mkdir new_folder; dst
umount /media/test_fs

dd if=/dev/zero of=/home/mikogizka/Documents/disk_2 bs=500M count=1
mkfs.ext4 /home/mikogizka/Documents/disk_2
debugfs -w /home/mikogizka/Documents/disk_2
iii.write /etc/passwd file_1.txt
iv.write /etc/pam.conf file_2.txt
ls -l <no_inode> contoh: ls -l <2> atau perintah ls -l

head -c 1KB /dev/urandom > /tmp/test.txt
